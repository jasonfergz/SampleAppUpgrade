FRAMEWORK_SEARCH_PATHS = "$(PODS_ROOT)/couchbase-lite-ios"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Mantle" "${PODS_ROOT}/Headers/Public/couchbase-lite-ios" "${PODS_ROOT}/Headers/Public/couchbase-lite-ios/CouchbaseLite"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Mantle" -isystem "${PODS_ROOT}/Headers/Public/couchbase-lite-ios" -isystem "${PODS_ROOT}/Headers/Public/couchbase-lite-ios/CouchbaseLite"
OTHER_LDFLAGS = $(inherited) -ObjC -l"Mantle" -l"sqlite3" -l"z" -framework "CFNetwork" -framework "CouchbaseLite" -framework "Foundation" -framework "Security" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods